[speed]
label= MHz
command= lscpu | grep -i "cpu mhz" | cut -d " " -f27
interval= 1

[turbo]
label= NO-TURBO
command=cat /sys/devices/system/cpu/intel_pstate/no_turbo
interval=60

[separator]

