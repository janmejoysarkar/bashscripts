full_text=〉
align=left
separator=false
separator_block_width=3

[disk]
label=📀
command=df -h | grep "/$" | awk '{print $5}'
interval=600

[separator]

[volume]
label=🔊
command=$HOME/.config/i3blocks/volume/volume
interval=1

[separator]

[conservation_mode]
command=$HOME/.config/i3blocks/custom/batteryCmode_check.sh
interval=30

[battery2]
command=$HOME/.config/i3blocks/battery/battery
interval=30

[separator]

[time]
label=⏰
command=date '+%a, %b %d %H:%M:%S'
interval=5

[separator]

[bluetooth]
command=$HOME/.config/i3blocks/custom/bluetooth.sh
interval=3

